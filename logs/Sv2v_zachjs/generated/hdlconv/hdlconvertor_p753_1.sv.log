module gray2bin1 (
	bin,
	gray
);
	parameter SIZE = 8;
	output [(SIZE - 1):0] bin;
	input [(SIZE - 1):0] gray;
	genvar i;
	generate
		for (i = 0; (i < SIZE); i = (i + 1)) begin : bitnum
			assign bin[i] = (^ gray[(SIZE - 1):i]);
		end
	endgenerate
endmodule
