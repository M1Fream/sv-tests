module mux_using_if (
	din_0,
	din_1,
	sel,
	mux_out
);
	input din_0;
	input din_1;
	input sel;
	output mux_out;
	reg mux_out;
	always @(sel or din_0 or din_1) begin : MUX
		if ((sel == 1'b0)) begin
			mux_out = din_0;
		end
		else begin
			mux_out = din_1;
		end
	end
endmodule
