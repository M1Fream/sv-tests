module aes_key_expand_128 (
	clk,
	kld,
	key,
	wo_0,
	wo_1,
	wo_2,
	wo_3
);
	input clk;
	input kld;
	input [15:0] key;
	output [3:0] wo_0;
	output [3:0] wo_1;
	output [3:0] wo_2;
	output [3:0] wo_3;
	reg [15:0] w;
	assign wo_0 = w[0+:4];
	assign wo_1 = w[4+:4];
	assign wo_2 = w[8+:4];
	assign wo_3 = w[12+:4];
	always @(posedge clk) begin
		w[0+:4] <= (kld ? key[15:12] : w[0+:4]);
		w[4+:4] <= (kld ? key[11:8] : (w[0+:4] ^ w[4+:4]));
		w[8+:4] <= (kld ? key[7:4] : ((w[0+:4] ^ w[4+:4]) ^ w[8+:4]));
		w[12+:4] <= (kld ? key[3:0] : (((w[0+:4] ^ w[4+:4]) ^ w[8+:4]) ^ w[12+:4]));
	end
endmodule
