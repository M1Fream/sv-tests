module top;
	wire [1:0] a;
	reg b;
	initial begin
		;
		(* full_case, parallel_case *)
		case (a)
			2'b00: b = 0;
			2'b01, 2'b10: b = 1;
			default: b = 0;
		endcase
		(* full_case = 1 *)
		(* parallel_case = 1 *)
		case (a)
			2'b00: b = 0;
			2'b01, 2'b10: b = 1;
			default: b = 0;
		endcase
		(* full_case, parallel_case = 0 *)
		case (a)
			2'b00: b = 0;
			2'b01, 2'b10: b = 1;
			default: b = 0;
		endcase
	end
endmodule
